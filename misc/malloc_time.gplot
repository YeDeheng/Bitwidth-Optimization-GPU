#set term postscript eps enhanced 
set term png enhanced
#set output 'out.pdf'
#set output '| epstopdf --filter --outfile=plot.pdf'
set size 0.8,0.6
set xtics nomirror # rotate by -30
set ytics nomirror
set grid

set ylabel 'average time (ms)'
set xlabel 'BITWIDTH SPACE = pow(2,i)'
#set logscale x
set xrange [0:30]
#set xtics 
set datafile separator ","

plot   'malloc_time.dat' using 1:3 with linespoints lw 2 lc 7 lt 1 title 'average time per bitwidth combination'; 
